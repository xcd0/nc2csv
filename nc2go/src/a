func (p *program) run(pc int) {
	if pc == 0 || pc > p.length {
		e := fmt.Sprintf("実行エラー : l.%d : その行は存在しません。エラーです。", pc)
		log.Fatal(e)
	}

	NOP := false // コメント行 runBlock()を呼ばない
	switch pc {
	case 1:
		NOP = true
	case 2:
		Assign(O, 1)
	case 3:
		EnqueueForG(49)
	case 4:
		EnqueueForG(69)
	case 5:
		Assign(M, 72)
	case 6:
		Assign(M, 69)
	case 7:
		Assign(M, 3)
		Assign(S, 1000)
	case 8:
		EnqueueForG(90)
		EnqueueForG(00)
	case 9:
		EnqueueForG(90)
		EnqueueForG(10)
		Assign(L, 11)
		Assign(P, Hash[4120])
		Assign(R, -1.0)
	case 10:
		Assign(X, 0.0)
		Assign(Y, 0.0)
	case 11:
		EnqueueForG(43)
		Assign(Z, 50.0)
	case 12:
		EnqueueForG(90)
	case 13:
		EnqueueForG(1)
		Assign(X, -6.0)
		Assign(Y, 0.0)
		Assign(Z, 50.0)
		Assign(A, -90.0)
		Assign(F, 1000)
	case 14:
		EnqueueForG(00)
		Assign(Z, 31.0)
		Assign(A, -90.0)
	case 15:
		EnqueueForG(1)
		Assign(X, -3.0)
		Assign(F, 5000)
	case 16:
		Assign(X, 0.0)
	case 17:
		Assign(Z, 26.0)
		Assign(F, 1000)
	case 18:
		Assign(A, -90.912)
		Assign(F, 2204)
	}
	if !NOP {
		runBlock()
	}
}

